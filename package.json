{
  "name": "flowmap.query-backend",
  "version": "1.0.0",
  "description": "",
  "main": "backend/server.js",
  "scripts": {
    "start": "env $(cat .env) NODE_ENV=production node backend/server.js",
    "build-client": "cd client && npm i && npm run build",
    "build": "npm run build-client",
    "dev:client": "cd client && npm start",
    "dev:server": "env $(cat .env) nodemon backend/server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run dev:server\" \"npm run dev:client\"",
    "docker": "docker build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  },
  "dependencies": {
    "csv-parse": "^4.4.6",
    "csv-stringify": "^3.1.1",
    "d3-dsv": "^1.0.10",
    "js-yaml": "^3.13.1",
    "koa": "^2.5.3",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "^3.0.0",
    "koa-json": "^2.0.2",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-static": "^5.0.0",
    "node-fetch": "^2.6.7",
    "ramda": "^0.25.0",
    "sqlstring": "^2.3.1"
  }
}
